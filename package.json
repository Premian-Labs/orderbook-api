    "js-yaml": "^4.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript-json-schema": "^0.62.0"
  },
  "engines": {
    "node": ">=18"
  }
