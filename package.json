    "@types/node": "^18.13.0",
    "@types/ws": "^8.5.4",
    "@unkey/api": "^0.11.0",
    "ajv": "^8.12.0",
    "axios": "^1.5.0",
    "dotenv": "^16.0.3",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "typescript": "^5.2.2",
    "web3-utils": "^1.9.0",
    "winston": "^3.9.0",
    "ws": "^8.12.1"
