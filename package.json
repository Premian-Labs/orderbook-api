  "license": "BSD-3-Clause",
  "keywords": [
    "premia",
    "v3",
    "sdk",
    "api",
    "ethereum",
    "defi",
    "options",
    "trading",
    "and",
    "market-making"
  ],
  "scripts": {
    "build": "tsc --build",
    "start": "node ./dist/index.js",
    "prettier": "./node_modules/.bin/prettier --write --ignore-path .gitignore ./src/**",
    "e2e:account": "mocha --require ts-node/register test/account.ts --timeout 20000",
